/*
	Used Colors:
*/

$MainColor_norm: #c8bb89;
$MainColor_dark: #393939;
$MainColor_light: #FFFFFF;

$WarnColor: #D60000;


.commentItem .body,
.commentable_text .bodytext {
  font-size: 110%;
  line-height: 1.7em;
}
.red {
  background-color: #ffe500 !important;
}
.savedComment {
  background-color: $MainColor_light;
}

.savedComment.current *,
.savedComment.current {
  background-color: yellow;
}

.savedComment.current .footnote {
  background-color: $MainColor_light !important;
}
.savedComment.current .footnote a {
  background-color: transparent !important;
}

.comment-new {
  background-color: lightBlue;
}

.offscreen {
  position: absolute;
  left:-5000px;
}

.unselectable,
.unselectable *{
    -moz-user-select: -moz-none;
    -khtml-user-select: none;
    -webkit-user-select: none;

    /*
      Introduced in IE 10.
      See http://ie.microsoft.com/testdrive/HTML5/msUserSelect/
    */
    -ms-user-select: none;
    user-select: none;
}
.selectable,
.selectable *{
    -moz-user-select: text;
    -khtml-user-select: auto;
    -webkit-user-select: auto;

    /*
      Introduced in IE 10.
      See http://ie.microsoft.com/testdrive/HTML5/msUserSelect/
    */
    -ms-user-select: auto;
    user-select: auto;
}

.text_comments {
  position: absolute;
  left: 454px;
  top: -115px;
  width: 500px;
}

.text_comment_list {
  list-style-type: none;
}

.commentItem {
	position: absolute;
	display: block;
	background: transparent;
	width: 403px;
	z-index: 20;
	padding-bottom: 0;
	border-color: transparent;
	border-width: 2px;
	border-style: solid;
  
	&.fixed {
		position: fixed;
		top: 0;
		z-index: 900;
	}
	
	&.hovering {
		z-index: 901;
		border-color: #ffe501;
		-webkit-box-shadow: 0px 0px 40px 1px rgba(255,229,1,0.5);
		box-shadow: 0px 0px 40px 1px rgba(255,229,1,0.5);

		-moz-border-radius: 12px 0px 0px 0px;
		-webkit-border-radius: 12px 0px 0px 0px;
		-khtml-border-radius: 12px 0px 0px 0px;
		border-radius: 12px 0px 0px 0px;
		margin-top: 0px;
		margin-left: 0px;
	}

	&.newest {
		border: 2px solid #239b3a;
		border-radius: 14px 0px 0px 0px;
	}
	
	&.hoverOffset {
		margin-top:20px;
		margin-left: -20px;
	}
	
	&.headerTop {
		background: #239b3a;
		width: 375px;
		height: 40px;
		z-index: 9;
		margin-bottom: -10px;
		color: $MainColor_light;
		font-size:1.3em;
	}
	
	.header {
		border-bottom: 1px solid #dcdcdc;
		height: 47px;
		
		.logo,
		.user-verified,
		.user {
			float: left;
			display: block;
			line-height: 38px;
			padding: 0 5px;
		}
	}
	
	.body {
		padding: 10px;
		min-height: 50px;
		background: #f2f8f2;
	}
	
	.footer {
		background: #cce8be;
		padding: 5px 20px;
	}
	
	.button {
		padding-left: 9px;
		padding-right: 42px;
		background: #f2f8f2 url('../img/vote_button.png') no-repeat;
		line-height: 31px;
		display: inline-block; 
		font-weight: bold;
		border-radius: 3px;
		cursor:pointer;
		
		&.uservote {
			background-color: #f2f8f2 !important;
			cursor: default;
		}
		
		&.like {
			color: green;
			background-position: -39px -2px ;
			
			&:focus,
			&:hover {
				background: #269c3a url('../img/vote_button.png') no-repeat;
				background-position: -39px -50px ;
				color: $MainColor_light;
				text-decoration: none;
			}
			
			&.uservote {
				background-position: -39px -92px ;
				color: green;
				
				&:focus,
				&:hover {
					background-position: -39px -92px ;
				}
			}
		}
		
		&.dislike {
			color: red;
			background-position: 23px -2px ;
			
			&:focus,
			&:hover {
				background: #269c3a url('../img/vote_button.png') no-repeat;  
				background-position: 23px -50px ;
				color: $MainColor_light;
				text-decoration: none;
			}
			
			&.uservote {
				color: red;
				background-position: 23px -92px ;
				
				&:focus,
				&:hover {
					background-position: 23px -92px ;
				}
			}
		}
		
	}
	
	.edit .button {
		background: #f2f8f2;
		color: #005fa1;
		padding-right: 9px;
	}
	
	.close {
		text-indent: -5000px;
		background: url('../img/comment_close.png') no-repeat top left;
		float: right;
		width: 20px;
		height: 21px;
		cursor: pointer;
		
		&:focus,
		&:hover {
			background-position: 0 -23px;
		}
		
	}
	
} // .commentItem


.comment_ui,
.commentItem .headerTop,
.commentItem .headerContainer {
  padding: 10px 15px 0;
  -moz-border-radius: 10px 0px 0px 0px;
  -webkit-border-radius: 10px 0px 0px 0px;
  -khtml-border-radius: 10px 0px 0px 0px;
  border-radius: 10px 0px 0px 0px;
  background: $MainColor_norm;
  position: relative;
  z-index: 10;
}


.user-verified {
  width: 63px;
  height: 38px;
  background: url('../img/bbt-verified.png') no-repeat center center;
}

.footer {
	height: 31px;
	position: relative;

	.makeVote,
	.commentVotes {
		position: absolute;
	}
	
	.commentVotes {
		top: 0;
		right: 212px;
		line-height: 42px;
		background: url('../img/bbt-trenner.png') no-repeat right center;
		padding-right: 10px;
	}
	
	.makeVote{
		top: 5px;
		right: 11px;
	}
	
	.show,
	.edit {
		line-height: 33px;
	}
	
	.comment-chars-status {
		float: right;
	}
	
	.error {
		color: $WarnColor;
	}
	
}


.voteValue {
  padding: 0 5px;
  font-size: 1.2em;
  background: $MainColor_light;
  border:5px solid #91b691;
  -moz-border-radius: 12px;
  -webkit-border-radius: 12px;
  -khtml-border-radius: 12px;
  border-radius: 12px;
  font-weight: bold;
  color: #005fa1;
}

.savedComment .footnote {
  padding: 0px 2px;
  background: $MainColor_light;
  border:2px solid #91b691;
  -moz-border-radius: 7px;
  -webkit-border-radius: 7px;
  -khtml-border-radius: 7px;
  border-radius: 7px;
  display: inline-block;
  cursor: pointer;
  font-size: 12px;
  line-height: 13px;
}

.voteValue_small {
  padding: 2px;
  background: $MainColor_light;
  border:2px solid #91b691;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -khtml-border-radius: 5px;
  border-radius: 5px;

}

.savedComment .acceptanceLevel-1,
.acceptanceLevel-1 {
    border-color: #cc0000;
}

.savedComment .acceptanceLevel-2,
.acceptanceLevel-2 {
    border-color: #b97f7b;
}

.savedComment .acceptanceLevel-3,
.acceptanceLevel-3 {
    border-color: #cbcbcb;
}

.savedComment .acceptanceLevel-4,
.acceptanceLevel-4 {
    border-color: #aabda5;
}

.savedComment .acceptanceLevel-5,
.acceptanceLevel-5 {
    border-color: #259b3a;
}



