  padding:0;
}

li.pstepItem {
  padding-right: 60px;
  padding-bottom:0;
  position: relative;
  background: url('../img/bbt-psteps.png') no-repeat 144px 51px;
}

li.pstepItem.pastStep{
  background-position: 144px -179px;
} 

li.pstepItem.lastStep{
  background:transparent;
} 

