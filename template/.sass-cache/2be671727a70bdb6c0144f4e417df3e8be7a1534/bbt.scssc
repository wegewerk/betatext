.bbt-login-form {
	padding: 20px;
	background: $MainColor_norm;
	border-radius: 14px 0 0 0;  
	
	input {
		width: 100%;
	}
	
	h2 {
		background: url("../img/bg_dark_grey_line.png") no-repeat scroll center bottom transparent;
